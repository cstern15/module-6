#Problem 2
from datetime import datetime  
from datetime import timedelta  
#subtract 60 seconds  
print datetime.now() - timedelta(seconds=60)
#will print the time and date 60 seconds in the past

from datetime import datetime  
from datetime import timedelta  
#add 2 years, assuming no leap year so 1 year is 365 days
print datetime.now() + timedelta(days=730)
#will print the time and date 2 years in the future

#Problem 3
#timedelta object,d, representing 100 days, 10 hours, 13 minutes
from datetime import timedelta
d = timedelta(100 days, 10 hours, 13 minutes)
print(d)

